delete_row <- function(x, row_index) {
if (!inherits(x, "flextable")) {
stop("Function `delete_row` supports only flextable objects.")
}
if (row_index <= 0 || row_index > nrow(x[["body"]][["dataset"]])) {
stop("Row index out of bounds.")
}
part <- "body"
x[[part]] <- complex_tabpart(
data = x[[part]]$dataset[-row_index, , drop = FALSE], # Remove the specified row
col_keys = x$col_keys,
cwidth = x[[part]]$colwidths, cheight = x[[part]]$rowheights
)
x
}
